{
  "logger": {
      "excludePaths": ["/", "/ping", "/metrics"],
      "console": {
          "enabled": true
      },
      "file": {
          "enabled": true,
          "path": "logs/logs.txt",
          "interval": "day"
      },
      "seq": {
          "enabled": true,
          "url": "http://seq:5341",
          "apiKey": "secret"
      }
  },
  "metrics": {
      "enabled": true,
      "influxEnabled": false,
      "prometheusEnabled": true,
      "influxUrl": "http://influx:8086",
      "database": "swiftparcel",
      "env": "local",
      "interval": 5
  }
}